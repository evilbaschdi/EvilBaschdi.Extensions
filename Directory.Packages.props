    <PackageVersion Include="EvilBaschdi.Testing" Version="2024.1.5.633-develop" />
    <PackageVersion Include="FluentAssertions.Analyzers" Version="0.29.0" />
    <PackageVersion Include="Meziantou.FluentAssertionsAnalyzers" Version="1.0.19" />
    <PackageVersion Include="Microsoft.NET.Test.Sdk" Version="17.9.0-preview-23577-04" />
